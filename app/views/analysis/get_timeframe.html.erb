<%= form_with scope: :timeframe, url: {controller: :analysis, action: :set_timeframe}, method: :put do |form| %>
  <%= form.datetime_select 't_start', order: [:day, :month, :year, :hour, :minute], use_two_digit_numbers: true %>
  <%= form.datetime_select 't_end', order: [:day, :month, :year, :hour, :minute], use_two_digit_number: true %>
  <%= form.submit 'Set Timeframe' %>
<% end %>
